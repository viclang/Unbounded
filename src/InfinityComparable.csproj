<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
      <TargetFramework>net6.0</TargetFramework>
      <ImplicitUsings>enable</ImplicitUsings>
      <Nullable>enable</Nullable>
	  <id>InfinityComparable</id>
	  <version>1.0.0</version>
	  <authors>Victor Langemaire</authors>
	  <PackageLicenseExpression>MIT</PackageLicenseExpression>
	  <projectUrl>https://github.com/viclang/InfinityComparable</projectUrl>
	  <requireLicenseAcceptance>false</requireLicenseAcceptance>
	  <description></description>
	  <releaseNotes></releaseNotes>
	  <copyright></copyright>
      <RepositoryType>git</RepositoryType>
      <GenerateAssemblyInfo>false</GenerateAssemblyInfo>
      <PublishRepositoryUrl>true</PublishRepositoryUrl>
      <IncludeSymbols>true</IncludeSymbols>
      <PackageProjectUrl>https://github.com/viclang/InfinityComparable</PackageProjectUrl>
      <PackageReadmeFile>README.md</PackageReadmeFile>
  </PropertyGroup>	
</Project>
